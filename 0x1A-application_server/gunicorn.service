description "Gunicorn Application Server that runs Airbnb_v4"
author "Muhammad Ibrahim B"

start on runlevel [2345]
stop on runlevel [!2345]

respawn
setuid ubuntu
setgid www-data

chdir /home/ubuntu/AirBnB_clone_v4
exec guinicon --workers 3 --bind 0.0.0.0:5003 web_dynamic.2hbnb:app >> /tmp/airbnb-access.log 2> /temp/airbnb-error.logaorbnb-access.log
